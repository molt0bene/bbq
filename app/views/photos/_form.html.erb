<%= form_for [@event, photo], data: { turbo:false }, html: { multipart: true } do |f| %>
  <%= render partial: 'partials/form_errors', locals: {model: photo} %>

  <div class="form-group">
    <%= f.label :photo %>
    <%= f.file_field :photo %>
  </div>

  <div class="actions">
    <%= f.submit "#{I18n.t('forms.buttons.upload')}", class: 'btn btn-primary' %>
  </div>
<% end %>
